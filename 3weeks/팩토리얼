namespace Recursive01
{
    internal class Program
    {

        static int Factorial(int n)
        {
            if (n == 1)
                return n;
            else 
                return n *Factorial(n - 1);
        }
        static void Main(string[] args)
        {
            int a = 4;
            Console.WriteLine(Factorial(a));
        }
    }
}
