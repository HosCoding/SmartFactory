--시퀸스 만들기
CREATE SEQUENCE SEQ_DEPT_SEQUENCE
INCREMENT BY 10
START WITH 10
MAXVALUE 90
MINVALUE 0
NOCYCLE
CACHE 2;

--------------------

INSERT INTO DEPT_SEQUENCE (DEPTNO, DNAME, LOC)
VALUES (seq_dept_sequence.nextval, 'DATABASE', 'ANDONG');

--------------------

SELECT * FROM DEPT_SEQUENCE;

--------------------


SELECT SEQ_DEPT_SEQUENCE.CURRVAL FROM DUAL;
