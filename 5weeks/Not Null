CREATE TABLE TABLE_NOTNULL(
LOGIN_ID  VARCHAR2(20) NOT NULL,
LOGIN_PWD VARCHAR2(20) NOT NULL,
TEL       VARCHAR2(20)
);

---------------

DESC TABLE_NOTNULL;

----------------

INSERT INTO TABLE_NOTNULL
VALUES ('TEST', NULL, '010-1234-5678');

---------------

INSERT INTO TABLE_NOTNULL
VALUES ('TEST', 'TEST11', '010-1234-5678');

-----------------

SELECT *FROM TABLE_NOTNULL;
