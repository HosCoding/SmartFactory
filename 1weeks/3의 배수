namespace Quiz5_2
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int i = 0;
            int j = 0;
            int k = 0;

            while (i <= 100)
            {
                i++;
                if (i % 3 == 0)
                Console.WriteLine($"{i}");
            }
           
        }
    }
