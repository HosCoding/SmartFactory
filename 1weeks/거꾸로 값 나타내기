using System.Net.Security;

namespace ExamAPP01
{
    internal class Program
    {
        static void Main(string[] args)
        {
            for (int i = 100; i >= 0; i--)
            {
                if(i % 2 == 0)
                {

                    Console.Write($"{i} ");
                }

            }


        }
    }
}
